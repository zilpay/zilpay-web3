  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "serve": "http-server ./"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/big.js": "^6.2.2",
    "@types/bn.js": "^5.1.5",
    "@types/bun": "latest",
    "http-server": "^14.1.1",
    "rollup": "^4.18.0",
