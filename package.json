{
  "name": "@zilpay/zilpay-web3",
  "version": "0.0.1",
  "repository": "git@github.com:zilpay/zilpay-web3.git",
  "author": "Rinat <lich666black@gmail.com>",
  "type": "module",
  "license": "MIT",
  "types": "dist/index.d.ts",
